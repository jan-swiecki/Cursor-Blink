{
  "name": "Remove Blink",
  "description" : "Removes cursor blink in Google Docs.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["https://docs.google.com/*"],
  "browser_action": {
    "default_icon": "eyeball.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens blink"
    }
  },
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
	"css": ["styles.css"]
  }]
}
