{
  "manifest_version": 3,
  "name": "Remove Blink",
  "description": "Removes cursor blink in Google Docs.",
  "version": "1.0",

  "action": {
    "default_icon": "eyeball.png"
  },

  "host_permissions": [
    "https://docs.google.com/*"
  ],

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}
